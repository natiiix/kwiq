{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": true,
        "no-console": false,
        "quotemark": [
            true,
            "double",
            "avoid-escape"
        ],
        "no-any": true,
        "no-var-keyword": true,
        "typedef": [
            true,
            "call-signature",
            "arrow-call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration",
            "variable-declaration",
            "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ]
    },
    "rulesDirectory": []
}
